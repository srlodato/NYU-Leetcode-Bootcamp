class Solution:
    def twoSum(self, nums, target):
        l = 0
        r = len(nums) - 1

        while l < r:
            currSum = nums[l] + nums[r]
            
            if currSum == target:
                return [l + 1, r + 1]
            elif currSum < target:
                l += 1
            else:
                r -= 1
